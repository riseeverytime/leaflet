{
  "presets": ["@babel/preset-react", "@babel/preset-typescript"],
  "plugins": ["dev-expression"],
  "env": {
    "development": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "corejs": 3,
            "loose": true,
            "modules": "commonjs",
            "targets": {
              "node": "current",
              "browsers": "last 2 versions"
            },
            "useBuiltIns": "usage"
          }
        ]
      ],
      "plugins": [["@babel/transform-runtime", { "corejs": 3 }]]
    },
    "rollup": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "corejs": 3,
            "loose": true,
            "modules": false,
            "targets": {
              "node": "current",
              "browsers": "last 2 versions"
            },
            "useBuiltIns": "usage"
          }
        ]
      ],
      "plugins": [["@babel/transform-runtime", { "corejs": 3 }]]
    },
    "esm": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "corejs": 3,
            "loose": true,
            "modules": false,
            "targets": {
              "node": "current",
              "browsers": "last 2 versions"
            },
            "useBuiltIns": "usage"
          }
        ]
      ],
      "plugins": [
        [
          "@babel/plugin-transform-runtime",
          { "corejs": 3, "useESModules": true }
        ]
      ]
    }
  }
}
